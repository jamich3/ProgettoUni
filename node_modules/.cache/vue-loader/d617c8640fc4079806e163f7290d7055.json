ute.params.id).then((data) => {\r\n        this.person = data.data;\r\n      });\r\n      DataService.getStats(this.$route.params.id).then((stats) => {\r\n        this.stats = stats;\r\n      });\r\n    },\r\n    change: function () {\r\n      DataService.vote(this.$route.params.id, this.rating);\r\n    },\r\n    loadVote: function () {\r\n      return this.stats;\r\n    },\r\n  },\r\n  created: function () {\r\n    this.load();\r\n  },\r\n  watch: {\r\n    $route: function () {\r\n      this.load();\r\n    },\r\n  },\r\n};\r\n</script>"]}]}