">\r\n      <v-app-bar-nav-icon @click.stop=\"drawer = !drawer\"></v-app-bar-nav-icon>\r\n      <v-toolbar-title>FOOD<b>PROG</b>2.0</v-toolbar-title>\r\n      <v-spacer></v-spacer>\r\n      <v-text-field\r\n        hide-details\r\n        solo-inverted\r\n        flat\r\n        prepend-inner-icon=\"mdi-magnify\"\r\n        label=\"Cerca prodotto...\"\r\n      ></v-text-field>\r\n    </v-toolbar>\r\n     </v-row>\r\n<!--    <v-toolbar class=\"md-primary\">\r\n      <v-button class=\"md-icon-button\" @click=\"showNavigation = true\">\r\n        <v-icon class=\"fa fa-bars\"></v-icon>\r\n      </v-button>\r\n             <router-link :to=\"'/'\">\r\n          <span class=\"md-title\">Activities</span>\r\n        </router-link>\r\n\r\n       <div class=\"md-toolbar-section-end\">\r\n       <md-autocomplete\r\n          md-layout=\"box\"\r\n          v-model=\"selectedActivity\"\r\n          :md-options=\"options\"\r\n          @md-changed=\"search\"\r\n          @md-selected=\"select\"\r\n        >\r\n          <label>Activity</label>\r\n        </md-autocomplete>\r\n      </div> \r\n    </v-toolbar>-->\r\n\r\n    <md-drawer :md-active.sync=\"showNavigation\" md-swipeable>\r\n      <md-toolbar class=\"md-transparent\" md-elevation=\"0\">\r\n        <span class=\"md-title\">Menu</span>\r\n      </md-toolbar>\r\n\r\n      <!-- Voci del drawer -->\r\n\r\n      <md-navigation-drawer app clipped v-model=\"drawer\">\r\n        <md-list v-for=\"(item, index) in drawerItems\" :key=\"index\">\r\n         <md-flex xs6>  \r\n            <md-subheader v-if=\"item.subheader\">\r\n              {{ item.subheader }}\r\n            </md-subheader>\r\n          </md-flex>  \r\n          <md-list-tile\r\n            v-for=\"(item, index) in item.tiles\"\r\n            :key=\"index\"\r\n            :to=\"item.route\"\r\n          >\r\n            <md-list-tile-action>\r\n              <md-icon>{{ item.icon }}</md-icon>\r\n            </md-list-tile-action>\r\n            <md-list-tile-content>\r\n              <md-list-tile-title>\r\n                {{ item.title }}\r\n              </md-list-tile-title>\r\n            </md-list-tile-content>\r\n          </md-list-tile>\r\n        </md-list>\r\n      </md-navigation-drawer>\r\n    </md-drawer>\r\n    <md-container>\r\n      <md-layout>\r\n        <md-flex mb-5>\r\n          <h1 class=\"headline\">{{ pageTitle }}</h1>\r\n        </md-flex>\r\n      </md-layout>\r\n      <md-layout row wrap>\r\n        <md-flex\r\n          xs12\r\n          sm4\r\n          lg3\r\n          xl2\r\n          v-for=\"(movie, index) in drinkList\"\r\n          :key=\"index\"\r\n          pb-3\r\n          pr-3\r\n        >\r\n          <List :drink=\"drink\" vertical style=\"height: 100%\" :key=\"index\" />\r\n        </md-flex>\r\n      </md-layout>\r\n    </md-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport List from \"./List\";\r\nexport default {\r\n  name: \"Temporary\",\r\n  components: {\r\n    List,\r\n  },\r\n  data() {\r\n    return {\r\n      showNavigation: false,\r\n      showSidepanel: false, // stringa di ricerca (di default è vuota)\r\n      drawerItems: {\r\n        // voci del menù\r\n        firstChunk: {\r\n          subheader: \"Voci del menu\",\r\n          tiles: [\r\n            {\r\n              title: \"Activities\",\r\n              icon: \"thumb_up\",\r\n              route: \"/activities\",\r\n            },\r\n            {\r\n              title: \"About\",\r\n              icon:  \"new_releases\",\r\n              route: \"/about\",\r\n            },\r\n          ],\r\n        },\r\n      },\r\n    };\r\n  },\r\n  computed: {\r\n    encodedQuery() {\r\n      return encodeURI(this.query.replace(/ /g, \"+\"));\r\n      // nella query dobbiamo sostituire gli spazi con il segno '+'\r\n      // e fare l'opportuna codifica\r\n    },\r\n  },\r\n  methods: {\r\n    search() {\r\n      this.$router.push(\"/search/\" + this.encodedQuery);\r\n      // quando l'utente preme invio nella barra di ricerca\r\n      // l'applicazione cambia route e inserisce come parametro\r\n      // la query, che sarà utilizzata dal componente per\r\n      // fare la richiesta API\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.page-container {\r\n  min-height: 300px;\r\n  overflow: hidden;\r\n  position: relative;\r\n  border: 1px solid rgba(#000, 0.12);\r\n}\r\n\r\n// Demo purposes only\r\n.md-drawer {\r\n  width: 230px;\r\n  max-width: calc(100vw - 125px);\r\n}\r\n\r\n.md-content {\r\n  padding: 16px;\r\n}\r\n</style>"]}]}